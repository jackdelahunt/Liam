fn range(start: u64, end: u64, step: u64): [u64] {
    insert "
        return (start..end).step_by(step as usize).collect();
    ";
    return [1, 2];
}

fn print_string(str: ^[char]): void {
    for c i in *str {
        print_char(*c);
    }
}

fn print_char(c: char): void {
    insert "print!(\"{}\", c as char);";
}

fn print_u64(n: u64): void {
    insert "println!(\"{}\", n);";
}