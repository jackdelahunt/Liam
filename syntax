MOV v r // move value into a register
where:
    v - value
    r - register

PRINT r  // print value
where:
    r - register

ADD r1 r2 or    // add to register values
where:
    r1 - register 1
    r2 - register 2
    or - output register

ALLOC s or  // allocate memory on the heap
where:
    s - size
    or - output register

PUT ar vr    // set a value on the heap
where:
    ar - address register
    vr - value register


GET ar or    // get a value on the heap
where:
    ar - address register
    vr - value register