import "stdlib/basic.liam";

struct Person {
	name: str,
	age: i64
}

fn main() void {
	let persons := []Person{
		new Person{name: "jack", age: 22},
		new Person{name: "liam", age: 26}
	};
	print_person_array(persons);
}

fn print_person_array(persons: ^.. Person) void {
	for let i := 0; i < ptr_len::<Person>(persons); i = i + 1; {
		println::<str>(persons[i].name);
		println::<i64>(persons[i].age);
	}
}