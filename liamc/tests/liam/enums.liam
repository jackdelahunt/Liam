//A
//B
//A != B
//A == A
//B == B

import "stdlib/basic.liam";

enum Foo {
    A,
    B
}

fn main() void {
    if make Foo::A == make Foo::A {
        println[str]("A");
    }

    if make Foo::B == make Foo::B {
        println[str]("B");
    }

    if make Foo::A != make Foo::B {
        println[str]("A != B");
    }

    let x := make Foo::A;
    if x == make Foo::A {
        println[str]("A == A");
    }

    let y := make Foo::A;
    y = make Foo::B;
    if y == make Foo::B {
        println[str]("B == B");
    }
}