import "../../stdlib";

struct Person {
    name: str,
    age: i64
}

struct People {
    one: Person,
    two: Person
}

fn main() void {
    let p: Person = new Person{name: "jack", age: 21};
    println::<str>(p.name);
    println::<i64>(p.age);

    let people: People = new People {
        one: new Person {name: "liam", age: 25},
        two: new Person {name: "mace window", age: 28}
    };

    println::<str>(people.one.name);
    println::<i64>(people.two.age);
}
