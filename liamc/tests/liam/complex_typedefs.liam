alias Literal as StringLiteral | NumberLiteral;

struct StringLiteral {
    s: str
}

struct NumberLiteral {
    n: i64
}

fn main() void {
    let l := get_literal();

    if l is StringLiteral as sl {
        println[str](sl.s);
    }
}

fn get_literal() Literal {
    return new StringLiteral{s: "Hello"};
}