import "stdlib/basic.liam";

struct Person {
    name: str,
    age: i64
}

struct People {
    one: Person,
    two: Person
}

fn main() void {
    let p: Person = make Person{name: "jack", age: 21};
    println[str](p.name);
    println[i64](p.age);

    let people: People = make People {
        one: make Person {name: "liam", age: 25},
        two: make Person {name: "mace window", age: 28}
    };

    println[str](people.one.name);
    println[i64](people.two.age);
}