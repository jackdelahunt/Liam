import "stdlib/basic.liam";

struct Person {
    name: str,
    age: s64
}

struct People {
    one: Person,
    two: Person
}

fn main(): void {
    let p: Person = new Person{"jack", 21};
    println[str](p.name);
    println[s64](p.age);

    let people: People = new People {
        new Person {"liam", 25},
        new Person {"mace window", 28}
    };

    println[str](people.one.name);
    println[s64](people.two.age);
}