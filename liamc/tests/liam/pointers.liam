//100
//this is a string
//this is a string

import "stdlib/basic.liam";

struct Pointers {
    s: str,
    s_ptr: ^str
}

fn main() void {
    let x := 100;
    let y := @x;
    println[i64](x);
    println[i64](*y);

    let s := "this is a string";
    let p := make Pointers{s: s, s_ptr: @s};
    println[str](s);

    // auto pointer dereference
    let ptr := @p;
    println[str](*ptr.s_ptr);
}
