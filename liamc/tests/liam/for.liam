import "stdlib/basic.liam";

fn main(): u64 {
    for let i: u64 = 0; i < 2; i = i + 1; {
        println[u64](i);
    }

    for let i := 10; i < 17; i = i + 2; {
        println[u64](i);
    }
 
    for let i: u64 = init(); compare(i); i = add(i); {
        println[u64](i);
    }
 
    return 0;
}

fn init(): u64 {
    return 0;
}

fn compare(n: u64): bool {
    return n < 2;
}

fn add(n: u64): u64 {
    return n + 1;
}