import "stdlib/basic.liam";

fn main() void {
    for let i := 0u64; i < 2u64; i = i + 1u64; {
        println[u64](i);
    }

    for let i := 10s64; i < 17; i = i + 2; {
        println[s64](i);
    }
 
    for let i: u64 = init(); compare(i); i = add(i); {
        println[u64](i);
    }
}

fn init() u64 {
    return 0u64;
}

fn compare(n: u64) bool {
    return n < 2u64;
}

fn add(n: u64) u64 {
    return n + 1u64;
}