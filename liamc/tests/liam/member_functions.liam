import "stdlib/basic.liam";

struct Foo {
    foo: s64
}

fn (Foo) print_foo() void {
    println[s64](self.foo);
}

fn (Foo) change_foo(n: s64) void {
    self.foo = self.foo + n;
    println[s64](self.foo);
}

fn main() void {
    let foo := new Foo{foo: 100};
    foo.print_foo();
    foo.print_foo();
    foo.change_foo(100);
    foo.change_foo(100);
}